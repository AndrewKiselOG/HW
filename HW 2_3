var a = 3
var b = 4
var c = 5
var d:Int
d = b * b - (4 * a * c)
if d > 0 {
    print("График скорее всего продолжит направление")
} else if  d < 0 {
    print("График скорее всего поменяет направление")
} else {
    print("Неопределенная ситуация — нужно ждать")
}
//меняю значния, чтобы разные результаты получать
a = 1
c = 4
d = b * b - (4 * a * c)
d > 0 ?  print("График скорее всего продолжит направление") : d < 0 ?  print("График скорее всего поменяет направление") : print("Неопределенная ситуация — нужно ждать")
a = 2
b = 7
d = b * b - (4 * a * c)
switch d {
case 1...:
    print("График скорее всего продолжит направление")
case ..<0:
    print("График скорее всего поменяет направление")
default:
    print("Неопределенная ситуация — нужно ждать")
}

var age:Int
var isFemale:Bool
age = Int.random(in:3...99)
isFemale = Bool.random()
switch age {
case 3...17:
    isFemale == true ? print("Детский контент для девочек") : print("Детский контент для мальчиков")
case 18...65:
    isFemale == true ? print("Взрослый контент для женщин") : print("Взрослый контент для мужчин")
case 66...:
    isFemale == true ? print("Контент для женщин пожилого возраста") : print("Контент для мужчин пожилого возраста")
default:
    break
}
